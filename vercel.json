{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/companies",
      "dest": "/api/companies.py"
    },
    {
      "src": "/api/stock/(.*)/summary",
      "dest": "/api/stock.py?path=$1/summary"
    },
    {
      "src": "/api/stock/(.*)",
      "dest": "/api/stock.py?path=$1"
    }
  ]
}
